{
  "welcomeFile": "/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "cnma_guest_srv_api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    },
    {
      "source": "^/api/(.*)$",
      "target": "/$1",
      "destination": "cnma_guest_srv_api",
      "authenticationType": "xsuaa",
      "csrfProtection": true
    },
    {
      "source": "^(.*)",
      "target": "cnmaguestportal/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    }
  ]
}
